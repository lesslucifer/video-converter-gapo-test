{
  "name": "video-converter",
  "version": "1.0.0",
  "description": "Video convert http server",
  "main": "dist/app.js",
  "scripts": {
    "int-test": "cross-env mocha -r node_modules/ts-node/register \"test/integration/**/*.ts\" --reporter list --exit",
    "test": "cross-env mocha -r node_modules/ts-node/register \"test/**/*.ts\" --reporter list --exit",
    "test-file": "cross-env mocha -r node_modules/ts-node/register --reporter list",
    "start": "npm run build && npm run watch",
    "build": "npm run build-ts",
    "serve": "nodemon dist/app.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "coverage": "nyc mocha \"test/**/*.ts\"",
    "swagger": "ts-node tasks/submit_swagger_to_s3.ts"
  },
  "author": "VuLQ",
  "license": "ISC",
  "dependencies": {
    "ajv": "^6.10.2",
    "amqplib": "^0.5.5",
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "express-router-ts": "0.0.15",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^7.0.0",
    "ioredis": "^4.14.1",
    "moment": "^2.29.4",
    "multer": "^1.4.2",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.12",
    "rsmq": "^0.11.0",
    "sha1": "^1.1.1",
    "typescript": "^3.7.4",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/amqplib": "^0.5.13",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.6",
    "@types/chai-as-promised": "^7.1.0",
    "@types/chai-http": "^3.0.5",
    "@types/chai-spies": "^1.0.0",
    "@types/express": "^4.11.1",
    "@types/fluent-ffmpeg": "^2.1.13",
    "@types/fs-extra": "^5.0.4",
    "@types/ioredis": "^4.14.3",
    "@types/lodash": "^4.14.108",
    "@types/mocha": "^5.2.5",
    "@types/multer": "^1.3.10",
    "@types/node": "^10.0.2",
    "@types/randomstring": "^1.1.6",
    "@types/sha1": "^1.1.1",
    "@types/uuid": "^3.4.6",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.2.0",
    "chai-spies": "^1.0.0",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "ts-node": "^7.0.1"
  }
}
